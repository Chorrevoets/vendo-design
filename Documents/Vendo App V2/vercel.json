{
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "installCommand": "npm install --legacy-peer-deps",
    "outputDirectory": ".next",
    "regions": [
        "iad1"
    ],
    "functions": {
        "app/**/*.tsx": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}